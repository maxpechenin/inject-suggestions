/*-- module:user-menu --*/
(function($){
    'use strict';
    var $userMenu = $('.user_menu');
            
    $('body').on('click','.header-personal__name', function(event){
        var menuState = event.currentTarget.classList.contains('open');
        if(menuState){
            $userMenu.removeClass('open');
        } else {
            $userMenu.addClass('open');
        }
        event.preventDefault();
    });
}(jQuery));


/*-- module:toggle Side menu --*/
(function($){
    'use strict';
  $('.side-navigation__primary-nav-alt li.active > a ').on('click', function(e){
    e.preventDefault();
    $(this).parent('li').toggleClass('active');
    $(this).parent('li').toggleClass('inactive');
    $(this).next('ul, div').toggle();
  });
}(jQuery));


/*-- module:toggle notification links --*/
(function($){
   'use strict';

    $('.header-personal__bubble').on('click', function(event){
        //event.preventDefault();
           $('.notification-show').toggleClass('notification-hidden');
     });

}(jQuery));


/*-- open external links in new window --*/
(function($){
   'use strict';

	$('body').on( 'click', 'a[href^=http]', function() {

		if (this.hostname != document.location.hostname) {
			event.preventDefault();
			window.open( $(this).attr('href') );
		}

	});

}(jQuery));

/*-- hide/display Event inputs for ics calendar file generation --*/
(function ($){
   'use strict';
	var tag =  $('#event-details-div');
	if(tag.length === 0) {return; }

	$('#metadata_field_select_8107').on('change', function(){
	  if( $(this).val() === "events" || $(this).val() === "training" || $(this).val()=== "infocus" || $(this).val()=== "social-club"){
	   $('#event-details-div').show();
	  }else{
	  $('#event-details-div').hide();
	  }
	});
	
}(jQuery));

/*-- Painful DE-Dupe--*/
(function ($) {
   'use strict';
	var tag =  $('.js-dedupe-directory');
	if(tag.length === 0) {return; }

        var orignal = [];
        $('div[data-userdupe]').each(function(){ 
         var current = $(this).attr('data-userdupe');

         if(orignal.indexOf(current) != -1){
           $(this).remove()}
         else{
           orignal.push(current);
         }
        });
}(jQuery));

/*-- Datepicker for creating Events --*/
(function ($) {
    $('#all-day-event').change(function(){
       var $checkbox = $(this);
        if($checkbox.prop('checked')){
            $('.event-time').hide(300);
            $('#metadata_field_date_19476_datetimevalue_h').val('0');
            $('#metadata_field_date_19476_datetimevalue_i').val('0');
            $('#metadata_field_date_19477_datetimevalue_h').val('0');
            $('#metadata_field_date_19477_datetimevalue_i').val('0');
        } else{
             $('.event-time').show(300);
        }
    });      

    $('#event-from').datepicker({
        changeMonth: true
        , minDate: 0
        , dateFormat: 'dd-mm-yy'
        , onClose: function (selectedDate) {
            $('#event-to').datepicker('option', 'minDate', selectedDate);
            var fromDateAr = selectedDate.split('-');

            $('#event-from-d').val(fromDateAr[0]);
            $('#event-from-m').val(fromDateAr[1]);
            $('#event-from-y').val(fromDateAr[2]);
            $('#event-from-h').val('0');
            $('#event-from-i').val('0');
            $('#event-from-s').val('00');

        }
    });
    $('#event-to').datepicker({
        changeMonth: true
        , dateFormat: 'dd-mm-yy'
        , onClose: function (selectedDate) {
            $('#event-from').datepicker('option', 'maxDate', selectedDate);
            var toDateAr = selectedDate.split('-');

            $('#event-to-d').val(toDateAr[0]);
            $('#event-to-m').val(toDateAr[1]);
            $('#event-to-y').val(toDateAr[2]);
            $('#event-to-h').val('0');
            $('#event-to-i').val('0');
            $('#event-to-s').val('00');
        }
    });
}(jQuery));
/*-- image uploader ajax --*/
(function ($){

	if ( $('#page_asset_builder_19647').length === 0 ){ return;}
//console.log('upload click1');
	$('#sq_upload_image_button').on('click', function(e){
//console.log('upload click2');
		e.preventDefault();
		var data = new FormData($('#page_asset_builder_19647')[0]);
		$.ajax({ 
                   url: 'http://intranet.dept.gov.au/_services/image-upload?',
         	   type: 'POST',
                   data: data,
                   processData: false,
                   contentType: false,
                   beforeSend:
                      function(){
                      $('#ImgUploadSpinner').show();
                   }
                   })
                    .then(
		     function(data){
                        $('#responseImagesURLsError').html("");
                        $('#ImgUploadSpinner').hide();
                        if ( data.responseAssetURL ){
                           $('.defaultImage').remove();
		           $('[name="image_0_926"]').val('').attr('val', ''); 
                           $('[name="image_0"]').val('').attr('val', ''); 
                           $('#responseImagesURLs').append("<li>" + data.responseAssetURL + "<p class='upload-preview' ><img  src='http://intranet.dept.gov.au/?a=" + data.responseAssetID + ":v1' /></p></li>"); 
                        }else{
                             $('#responseImagesURLsError').html("<p style='color: red;'>Image Upload Error<p>");
                        }
                  })
                  .fail(
		     function(data2){
$('#ImgUploadSpinner').hide()
errorMsg = data2.responseText.toString();
				errorMsg = errorMsg.substr(errorMsg .search('{"error'),errorMsg .length);
				errorMsg = errorMsg.substr(errorMsg.search("<ul><li>"),errorMsg.length);
				errorMsg = errorMsg.substr(0,errorMsg.search("</li></ul>"));
				$('#responseImagesURLsError').html("<div style='color: red;'>"+errorMsg+"</p>");
            })
	});
}(jQuery));




